{
    "name": "narrowspark/coding-standard",
    "description": "The Narrowspark Coding Standard for is a set of phpstan and php-cs-fixer rules applied to all Narrowspark projects.",
    "type": "library",
    "keywords": [
        "narrowspark",
        "cs",
        "cs-fixer",
        "config",
        "php-cs-fixer",
        "phpstan",
        "coding-standard"
    ],
    "license": "MIT",
    "homepage": "https://github.com/narrowspark/coding-standard",
    "authors": [
        {
            "name": "Daniel Bannert",
            "email": "d.bannert@anolilab.de",
            "homepage": "http://www.anolilab.de",
            "role": "Developer"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^7.2",
        "jwage/changelog-generator": "^1.2.0",
        "narrowspark/php-cs-fixer-config": "^4.0.0",
        "pepakriz/phpstan-exception-rules": "^0.8.2",
        "phpstan/phpstan": "^0.11.12",
        "phpstan/phpstan-deprecation-rules": "^0.11.2",
        "phpstan/phpstan-mockery": "^0.11.2",
        "phpstan/phpstan-phpunit": "^0.11.2",
        "phpstan/phpstan-strict-rules": "^0.11.1",
        "thecodingmachine/phpstan-strict-rules": "^0.11.2",
        "vimeo/psalm": "^3.4.0"
    },
    "autoload-dev": {
        "psr-4": {
            "": "bin"
        }
    },
    "config": {
        "sort-packages": true
    },
    "support": {
        "issues": "https://github.com/narrowspark/coding-standard/issues",
        "source": "https://github.com/narrowspark/coding-standard"
    },
    "require-dev": {
        "symfony/var-exporter": "^4.3.0"
    },
    "scripts": {
        "build": "\\CreatePhpCsFixerList::build"
    }
}
